{
  "name": "TG_OnlineTracker",
  "description": "TG_OnlineTracker",
  "keywords": ["TG_OnlineTracker", "telegram"],
  "repository": "https://github.com/InukaAsith/TG_OnlineTracker/",   
  "env": {
    "STRING_SESSION": {
      "description": "Pyrogram session string",
      "required": true
    },
    "USER": {
      "description": "Username (without @) or id of target user",
      "required": true
    },
    "DUMP_CHAT": {
      "description": "give channel t's username without @ or id which should use as logging chat",
      "required": true,
      "value": "me"
    },
   
    "APP_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },  
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "UPDATE_TIME": {
      "description": "update interval (sec.",
      "required": true,
      "value": "60"
    }
  }
}
